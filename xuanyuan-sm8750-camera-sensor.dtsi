&soc {
	led_flash_rear_wide: qcom,camera-flash0 {
		cell-index = <0>;
		compatible = "qcom,camera-flash";
		flash-source = <&pm8550_flash0 &pm8550_flash3>;
		torch-source = <&pm8550_torch0 &pm8550_torch3>;
		switch-source = <&pm8550_switch2>;
		status = "ok";
	};

	led_flash_rear_ultra: qcom,camera-flash1 {
		cell-index = <1>;
		compatible = "qcom,camera-flash";
		flash-source = <&pm8550_flash0 &pm8550_flash3>;
		torch-source = <&pm8550_torch0 &pm8550_torch3>;
		switch-source = <&pm8550_switch2>;
		status = "ok";
	};

	led_flash_rear_tele3x: qcom,camera-flash2 {
		cell-index = <2>;
		compatible = "qcom,camera-flash";
		flash-source = <&pm8550_flash0 &pm8550_flash3>;
		torch-source = <&pm8550_torch0 &pm8550_torch3>;
		switch-source = <&pm8550_switch2>;
		status = "ok";
	};

	led_flash_rear_tele5x: qcom,camera-flash3 {
		cell-index = <3>;
		compatible = "qcom,camera-flash";
		flash-source = <&pm8550_flash0 &pm8550_flash3>;
		torch-source = <&pm8550_torch0 &pm8550_torch3>;
		switch-source = <&pm8550_switch2>;
		status = "ok";
	};

	camera_wide_avdd_ldo: gpio-regulator@0 {
		compatible = "regulator-fixed";
		reg = <0x00 0x00>;
		regulator-name = "camera_wide_avdd_ldo";
		regulator-min-microvolt = <2900000>;
		regulator-max-microvolt = <2900000>;
		regulator-enable-ramp-delay = <100>;
		enable-active-high;
		gpio = <&tlmm 127 0>;
		pinctrl-names = "wide_avdd_active", "wide_avdd_suspend";
		pinctrl-0 = <&cam_sensor_wide_active_avdd
					 &cam_sensor_wide_suspend_avdd>;
	};

	camera_wide_avdd2_ldo: gpio-regulator@1 {
		compatible = "regulator-fixed";
		reg = <0x01 0x00>;
		regulator-name = "camera_wide_avdd2_ldo";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		regulator-enable-ramp-delay = <100>;
		enable-active-high;
		gpio = <&tlmm 119 0>;
		pinctrl-names = "wide_avdd2_active", "wide_avdd2_suspend";
		pinctrl-0 = <&cam_sensor_wide_active_avdd2
					 &cam_sensor_wide_suspend_avdd2>;
	};

	//Tele5X avdd
	camera_tele5x_avdd_ldo: gpio-regulator@5 {
		compatible = "regulator-fixed";
		reg = <0x05 0x00>;
		regulator-name = "camera_tele5x_avdd_ldo";
		regulator-min-microvolt = <2200000>;
		regulator-max-microvolt = <2200000>;
		regulator-enable-ramp-delay = <100>;
		enable-active-high;
		gpio = <&tlmm 126 0>;
		pinctrl-names = "tele5x_avdd_active", "tele5x_avdd_suspend";
		pinctrl-0 = <&cam_sensor_tele5x_active_avdd
					 &cam_sensor_tele5x_suspend_avdd>;
	};


	camera_tele3x_avdd_ldo: gpio-regulator@2 {
		compatible = "regulator-fixed";
		reg = <0x02 0x00>;
		regulator-name = "camera_tele3x_avdd_ldo";
		regulator-min-microvolt = <2800000>;
		regulator-max-microvolt = <2800000>;
		regulator-enable-ramp-delay = <100>;
		enable-active-high;
		gpio = <&tlmm 129 0>;
		pinctrl-names = "tele3x_avdd_active", "tele3x_avdd_suspend";
		pinctrl-0 = <&cam_sensor_tele3x_active_avdd
					 &cam_sensor_tele3x_suspend_avdd>;
	};

	camera_ois_wide_ldo: gpio-regulator@3 {
		compatible = "regulator-fixed";
		reg = <0x03 0x00>;
		regulator-name = "camera_ois_wide_ldo";
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		regulator-enable-ramp-delay = <100>;
		enable-active-high;
		gpio = <&tlmm 208 0>;
		pinctrl-names = "ois_wide_active", "ois_wide_suspend";
		pinctrl-0 = <&cam_ois_wide_active
					 &cam_ois_wide_suspend>;
	};

	qcom,cam-res-mgr {
		compatible = "qcom,cam-res-mgr";
		gpios-shared-pinctrl = <602 700 604 709  601 725>;
		shared-pctrl-gpio-names = "cam_wide", "cam_wide", "cam_tele3x", "cam_tele3x", "cam_tele5x", "cam_tele5x";
		pinctrl-names = "cam_wide_active", "cam_wide_suspend", "cam_tele3x_active", "cam_tele3x_suspend", "cam_tele5x_active", "cam_tele5x_suspend";
		pinctrl-0 = <&cam_sensor_wide_active_mclk0
					&cam_sensor_wide_active_rst0>;
		pinctrl-1 = <&cam_sensor_wide_suspend_mclk0
					&cam_sensor_wide_suspend_rst0>;
		pinctrl-2 = <&cam_sensor_mclk3_active
					&cam_sensor_tele3x_active_rst3>;
		pinctrl-3 = <&cam_sensor_mclk3_suspend
					&cam_sensor_tele3x_suspend_rst3>;
		pinctrl-4 = <&cam_sensor_tele5x_mclk0_active
		             &cam_sensor_tele5x_active_rst>;
		pinctrl-5 = <&cam_sensor_tele5x_mclk0_suspend
		             &cam_sensor_tele5x_suspend_rst>;
		status = "ok";
	};
};

&cam_cci0 {
	pinctrl-0 = <&xuanyuan_cci_i2c_scl0_active &xuanyuan_cci_i2c_sda0_active>;
	pinctrl-2 = <&xuanyuan_cci_i2c_scl1_active &xuanyuan_cci_i2c_sda1_active>;

	// wide actuator
	actuator_wide: qcom,actuator0 {
		cell-index = <0>;
		compatible = "qcom,actuator";
		cam_vaf-supply = <&L5M>;
		cam_vio-supply = <&L5N>;
		regulator-names = "cam_vaf", "cam_vio";
		rgltr-cntrl-support;
		rgltr-min-voltage = <3304000 1800000>;
		rgltr-max-voltage = <3304000 1800000>;
		rgltr-load-current = <150000 150000>;
		cci-master = <CCI_MASTER_1>;
		status = "ok";
	};

	// wide ois
	ois_wide: qcom,ois0 {
		cell-index = <0>;
		compatible = "qcom,ois";
		cam_vana-supply = <&camera_wide_avdd_ldo>;
		cam_vio-supply = <&L5N>;
		cam_vdig-supply = <&L1M>;
		cam_v_custom1-supply = <&camera_wide_avdd2_ldo>;
		cam_v_custom2-supply = <&camera_ois_wide_ldo>;
		cam_clk-supply = <&cam_cc_titan_top_gdsc>;
		cam_vaf-supply = <&L5M>;
		regulator-names = "cam_vana", "cam_vio", "cam_vdig",
					"cam_v_custom1", "cam_clk", "cam_vaf", "cam_v_custom2";
		rgltr-cntrl-support;
		rgltr-min-voltage = <2900000 1800000 856000 1800000 0 3304000 5000000>;
		rgltr-max-voltage = <2900000 1800000 856000 1800000 0 3304000 5000000>;
		rgltr-load-current = <150000 150000 150000 150000 0 150000 150000>;
		gpio-no-mux = <0>;
		use-shared-clk;
		gpios = <&tlmm 90 0>, <&tlmm 188 0>;
		gpio-reset = <1>;
		gpio-req-tbl-num = <0 1>;
		gpio-req-tbl-flags = <1 0>;
		gpio-req-tbl-label = "CAMIF_MCLK1", "CAM_RESET1";
		cci-master = <CCI_MASTER_1>;
		clocks = <&cambistmclkcc CAM_BIST_MCLK_CC_MCLK1_CLK>;
		clock-names = "cam_clk";
		clock-cntl-level = "nominal";
		clock-rates = <19200000>;
