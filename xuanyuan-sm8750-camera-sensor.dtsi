&pm8550_flash {
	status = "ok";
};

&pm8550_flash0 {
	qcom,max-current-ma = <750>;
	qcom,duration-ms = <300>;
	xiaomi,max-duration-ms = <300>;
};

&pm8550_flash3 {
	qcom,max-current-ma = <750>;
	qcom,duration-ms = <300>;
	xiaomi,max-duration-ms = <300>;
};

&pm8550_torch0{
	qcom,max-current-ma = <100>;
	qcom,ires-ua = <5000>;
};

&pm8550_torch3{
	qcom,max-current-ma = <100>;
	qcom,ires-ua = <5000>;
};

&pm8550_switch0 {
	qcom,led-mask = <1>;
	qcom,symmetry-en;
};

&pm8550_switch1 {
	qcom,led-mask = <8>;
	qcom,symmetry-en;
};

&pm8550_switch2 {
	qcom,led-mask = <9>;
	qcom,symmetry-en;
};

&i2c_freq_custom_cci0{
	hw-scl-stretch-en = <1>;
};

&tlmm {
