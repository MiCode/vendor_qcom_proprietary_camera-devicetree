		cam_vaf-supply = <&L7N>;
		cam_vdig-supply = <&camera_tele_dvdd_ldo>;
		cam_vana-supply = <&camera_tele_avdd_ldo>;
		cam_vio-supply = <&L23B>;
		cam_clk-supply = <&cam_cc_titan_top_gdsc>;
