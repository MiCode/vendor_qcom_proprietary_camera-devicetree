			pins = "gpio27";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
			output-low;
		};
	};
	//WIDE RST
	cam_sensor_wide_active_rst0: cam_sensor_wide_active_rst0 {
		mux {
			pins = "gpio29";
			function = "gpio";
		};

		config {
