&apps_rsc_drv2 {
	rpmh-regulator-ldon4 {
		compatible = "qcom,rpmh-vrm-regulator";
		qcom,resource-name = "ldon4";
		status = "ok";

		L4N: pm8010n_l4: vreg-pm8010n-l4 {
			regulator-name = "pm8010n_l4";
			qcom,set = <RPMH_REGULATOR_SET_ALL>;
			regulator-min-microvolt = <1792000>;
			regulator-max-microvolt = <3312000>;
			qcom,init-voltage = <1800000>;
		status = "ok";
		};
	};
};

&qupv3_se8_i2c {
	status = "ok";
	qcom,high-perf;
	qcom,clk-freq-out = <1000000>;
	qcom,pm-ctrl-client;
	// pinctrl-0 = <&luming_qupv3_se8_i2c_sda_active>, <&luming_qupv3_se8_i2c_scl_active>;
	// pinctrl-1 = <&luming_qupv3_se8_i2c_sda_suspend>, <&luming_qupv3_se8_i2c_scl_suspend>;

	// Front Sensor
	qcom,cam-sensor1 {
		cell-index = <1>;
		reg = <0x10>;
		compatible = "qcom,cam-i2c-sensor";
		csiphy-sd-index = <4>;
		eeprom-src = <&eeprom_front>;
		actuator-src = <&actuator_front>;
