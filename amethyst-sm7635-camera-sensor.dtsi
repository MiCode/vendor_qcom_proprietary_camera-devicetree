#include <dt-bindings/msm-camera.h>
&gcc {
	qcom,critical-devices = <&qupv3_se8_i2c &qupv3_se7_i2c>;
};
//bob
&qupv3_se8_i2c {
	#address-cells = <1>;
	#size-cells = <0>;
	status = "ok";

	wl2866d@28 {
		compatible = "xiaomi,wl2866d-pmic";
		reg = <0x28>;
		vin1-supply = <&S2B>;
		vin2-supply = <&BOB>;

		wl2866d_dvdd1: wl2866d-regulator@0 {
			cell-index = <0>;
			reg = /bits/ 8 <0x3>;
			type = "dvdd";
			regulator-name = "wl2866d_dvdd1";
			regulator-min-microvolt = <600000>;
			regulator-max-microvolt = <1800000>;
		};

		wl2866d_dvdd2: wl2866d-regulator@1 {
			cell-index = <1>;
			reg = /bits/ 8 <0x4>;
			type = "dvdd";
			regulator-name = "wl2866d_dvdd2";
			regulator-min-microvolt = <600000>;
			regulator-max-microvolt = <1800000>;
		};

		wl2866d_avdd1: wl2866d-regulator@2 {
			cell-index = <2>;
			reg = /bits/ 8 <0x5>;
			type = "avdd";
			regulator-name = "wl2866d_avdd1";
			regulator-min-microvolt = <1200000>;
			regulator-max-microvolt = <4300000>;
		};

		wl2866d_avdd2: wl2866d-regulator@3 {
			cell-index = <3>;
			reg = /bits/ 8 <0x6>;
			type = "avdd";
			regulator-name = "wl2866d_avdd2";
			regulator-min-microvolt = <1200000>;
			regulator-max-microvolt = <4300000>;
		};
	};
};
//hap
&qupv3_se7_i2c {
	#address-cells = <1>;
	#size-cells = <0>;
